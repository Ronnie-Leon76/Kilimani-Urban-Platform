{
  "buildCommand": "prisma generate && next build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": ["cle1"],
  "functions": {
    "app/api/auth/[...nextauth]/route.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true",
    "SKIP_ENV_VALIDATION": "true"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "true"
    }
  }
}
